{
	"svelte.plugin.css.diagnostics.enable": false,
	"css.validate": false,
	"scss.validate": false,
	"less.validate": false,
	"css.customData": [".vscode/tailwind.json"],
	"tailwindCSS.includeLanguages": {
		"svelte": "html"
	},
	"tailwindCSS.experimental.classRegex": [["class:([^=]+)={([^}]+)}", "[\"'`]([^\"'`]*).*?[\"'`]"]],
